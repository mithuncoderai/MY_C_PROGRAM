#include <stdio.h>
int main() {
    int marks;
    printf("Enter the student's marks\n ");
    scanf("%d", &marks);
    if (marks >= 90) {
        printf("Grade A\n");
    }
    else if (marks >= 75 && marks <= 89) {
        printf("Grade B\n");
    }
    else if (marks >= 60 && marks <= 74) {
        printf("Grade C\n");
    }
    else if (marks >= 50 && marks <= 59) {
        printf("Grade D\n");
    }
    else if (marks < 50 && marks >= 0) {
        printf("Grade F\n");
    }
    else {
        printf("the student marks=%d\n");
    }
}

