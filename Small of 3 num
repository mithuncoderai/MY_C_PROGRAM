#include <stdio.h>

int main() {
    int a, b, c, smallest;

    printf("Enter first number: ");
    scanf("%d", &a);

    printf("Enter second number: ");
    scanf("%d", &b);

    printf("Enter third number: ");
    scanf("%d", &c);

    if (a <= b && a <= c) {
        smallest = a;
    }
    else if (b <= a && b <= c) {
        smallest = b;
    }
    else {
        smallest = c;
    }

    printf("The smallest number is %d\n", smallest);

    return 0;
}
